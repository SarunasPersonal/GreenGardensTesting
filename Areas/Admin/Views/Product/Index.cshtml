@using GreenGardens.Models
@model List<Product>
<div class="container">
    <div class="row pt-5 pb-3">
        <div class="col-6">
            <h2 class="text-primary">
                Product List
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Product" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-database-add"></i> Add New Product
            </a>
        </div>
    </div>

<table id="tblData" class="table table-bordered table-striped style="width:100%">
    <thead>
    <tr>
       
        <th> Product Name</th>
        <th>SKU</th>
        <th>Manufacturer</th>
        <th>Price</th>
        <th>Category</th>
        
        
    </tr>
    </thead>
    
    
</table>
</div>
@section Scripts{
    <script src="~/js/Product.js"></script>
}
